}

#overlay {
   background: rgba(0,0,0,0.8);
   position:fixed;
   top: 0;
   right: 0;
   left:0;
   bottom: 0;
   width: 100%;
   height: 100%;
   z-index: 99999;
   display: none;
}

#overlay .modal {
   background: #fff;
   border-radius: 5px;
   width: 90%;
   margin: 100px auto;
   max-width: 800px;
   min-height: 100px;
